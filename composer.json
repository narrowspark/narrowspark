{
    "name": "narrowspark/narrowspark",
    "type": "library",
    "description": "Narrowspark framework for creative people.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio"
    ],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/narrowspark",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "cache/namespaced-cache": "^0.1",
        "doctrine/dbal" : "^2.5",
        "doctrine/migrations" : "^1.4",
        "narrowspark/framework": "dev-master",
        "roave/security-advisories": "dev-master",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.1",
        "fzaninotto/faker": "^1.6",
        "narrowspark/php-cs-fixer-config" : "^2.2",
        "phpunit/phpunit": "^6.0",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Migrations\\": "database/migrations/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "bin-dir": "vendor/bin",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php cerebro key:generate"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
