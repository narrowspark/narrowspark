{
    "name"       : "narrowspark/narrowspark",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["framework", "narrowspark", "symfony", "router", "brainwave"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/narrowspark",
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                           : ">=5.6",
        "narrowspark/framework"         : "0.9.8"
    },
    "require-dev": {
        "fzaninotto/faker"              : "~1.5",
        "mockery/mockery"               : "~0.9.4",
        "phpunit/phpunit"               : "~5.0.6",
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\"                     : "app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                : "1.0.2-dev"
        }
    },
    "config": {
        "bin-dir"                       : "vendor/bin"
    },
    "minimum-stability"                 : "dev",
    "prefer-stable"                     : true
}
