{
    "name": "narrowspark/narrowspark",
    "type": "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["framework", "narrowspark", "symfony", "router", "brainwave"],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/narrowspark",
    "authors": [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "narrowspark/framework"         : "0.9.8"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\"                     : "app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                : "1.0.0-dev"
        }
    },
    "suggest": {
        "intahwebz/lowmemoryclassloader": "Narrowspark has support for low memory classloader v1.0.1"
    },
    "config": {
        "bin-dir"                       : "vendor/bin"
    },
    "minimum-stability"                 : "dev",
    "prefer-stable"                     : true
}