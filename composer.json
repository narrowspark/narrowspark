{
    "name": "narrowspark/narrowspark",
    "type": "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["framework", "narrowspark", "slim", "router", "brainwave"],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/narrowspark",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role": "Developer"
        }
    ],
    "require": {
        "narrowspark/framework": "0.9.3.*@dev"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.3-dev"
        }
    },
    "scripts": {
        "post-autoload-dump": "php vendor/bin/classpreloader.php compile --config=/vendor/narrowspark/framework/src/Brainwave/Workbench/optimize.php --output=/storage/autoload/preload_autoload.php"
    },
    "config": {
        "bin-dir": "vendor\\bin"
    },
    "minimum-stability": "dev"
}