{
    "name": "narrowspark/narrowspark",
    "type": "library",
    "description": "Narrowspark framework for creative people.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio"
    ],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/narrowspark",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/dbal" : "^2.5",
        "doctrine/migrations" : "^1.4",
        "doctrine/orm" : "^2.5",
        "narrowspark/framework": "dev-develop",
        "roave/security-advisories": "dev-master",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "narrowspark/php-cs-fixer-config" : "^2.2",
        "narrowspark/installer": "dev-master",
        "phpunit/phpunit": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Migrations\\": "database/migrations/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "bin-dir": "vendor/bin",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "Narrowspark\\Installer\\OptionalPackages::install"
        ],
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
