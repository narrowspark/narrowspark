{
    "name": "narrowspark/narrowspark",
    "type": "library",
    "description": "Narrowspark framework for creative people.",
    "keywords": [
        "framework",
        "narrowspark",
        "viserio"
    ],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/narrowspark",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "7.0.0 - 7.0.5 || ^7.0.7",
        "roave/security-advisories": "dev-master",
        "narrowspark/framework": "dev-develop",
        "narrowspark/installer": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "narrowspark/testing-helper": "^1.5",
        "narrowspark/installer": "dev-master",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^5.1",
        "symfony/css-selector": "^3.1",
        "symfony/dom-crawler": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "config": {
        "bin-dir": "vendor/bin",
        "preferred-install": "dist"
    },
    "scripts": {
        "post-create-project-cmd": [
            "Narrowspark\\Installer\\OptionalPackages::install"
        ],
        "post-install-cmd": [
        ],
        "post-update-cmd": [
            "Narrowspark\\Installer\\OptionalPackages::install"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
